{
    "name": "telly-bot",
    "version": "0.0.8",
    "description": "A Telegram bot based on Koa.",
    "scripts": {
        "start": "nodemon --exec babel-node -- src/bin/www",
        "deploy": "npm run build && npm run pm2",
        "build": "babel src -d build",
        "pm2": "pm2 start process.json",
        "lint": "eslint --ignore-path .gitignore --ext .js .",
        "lint:fix": "npm run lint --fix",
        "lint:prettier": "prettier --ignore-path .gitignore --write **/* --ignore-unknown"
    },
    "author": "LolipopJ",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.20.7",
        "@octokit/core": "^4.1.0",
        "axios": "^1.2.2",
        "chance": "^1.1.9",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "js-yaml": "^4.1.0",
        "koa": "^2.14.1",
        "koa-bodyparser": "^4.3.0",
        "koa-router": "^12.0.0",
        "marked": "^4.2.5",
        "mdimg": "^0.2.0",
        "node-schedule": "^2.1.0",
        "node-telegram-bot-api": "^0.60.0",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
