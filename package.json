{
    "name": "telly-bot",
    "version": "0.0.8",
    "description": "A Telegram bot based on Koa.",
    "scripts": {
        "start": "nodemon --exec babel-node -- src/bin/www",
        "deploy": "npm run build && npm run pm2",
        "build": "babel src -d build",
        "pm2": "pm2 start process.json",
        "lint": "eslint --ignore-path .gitignore --ext .js .",
