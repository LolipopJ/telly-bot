{
    "name": "telly-bot",
    "version": "0.0.6",
    "description": "A Telegram bot based on Koa.",
    "scripts": {
        "start": "nodemon --exec babel-node -- src/bin/www",
        "deploy": "npm run build && npm run pm2",
        "build": "babel src -d build",
        "pm2": "pm2 start process.json",
        "lint": "eslint --ignore-path .gitignore --ext .js .",
        "lint:fix": "npm run lint --fix",
        "lint:prettier": "prettier --ignore-path .gitignore --write **/* --ignore-unknown"
    },
    "author": "LolipopJ",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.17.0",
        "@octokit/core": "^3.5.1",
        "axios": "^0.25.0",
        "debug": "^4.3.3",
        "dotenv": "^16.0.0",
        "js-yaml": "^4.1.0",
        "koa": "^2.13.4",
        "koa-bodyparser": "^4.3.0",
        "koa-router": "^10.1.1",
        "marked": "^4.0.12",
        "node-telegram-bot-api": "^0.56.0",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
        "pm2": "^5.1.2",
        "sequelize": "^6.15.0",
        "socks5-https-client": "^1.2.1",
        "toad-scheduler": "^1.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.0",
        "@babel/core": "^7.17.0",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/eslint-plugin": "^7.16.5",
        "@babel/node": "^7.16.8",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1"
    }
}
