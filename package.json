        "lint:prettier": "prettier --ignore-path .gitignore --write **/* --ignore-unknown"
    },
    "author": "LolipopJ",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.18.9",
        "@octokit/core": "^3.6.0",
        "axios": "^0.27.2",
        "chance": "^1.1.8",
        "debug": "^4.3.4",
        "dotenv": "^16.0.1",
        "js-yaml": "^4.1.0",
        "koa": "^2.13.4",
        "koa-bodyparser": "^4.3.0",
        "koa-router": "^12.0.0",
        "marked": "^4.0.18",
        "mdimg": "^0.2.0",
        "node-schedule": "^2.1.0",
        "node-telegram-bot-api": "^0.58.0",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
